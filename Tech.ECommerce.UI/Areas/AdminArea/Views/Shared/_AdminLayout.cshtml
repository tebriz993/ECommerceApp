<!DOCTYPE html>

<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>@ViewBag.Title</title>

    <link href="~/admintemplates/css/icons.min.css" rel="stylesheet" />
    <link href="~/AdminTemplates/libs/metrojs/release/MetroJs.Full/MetroJs.min.css" rel="stylesheet" type="text/css" />
    <link href="~/AdminTemplates/css/bootstrap.min.css" id="bootstrap-style" rel="stylesheet" type="text/css" />
    <!-- Icons Css -->
    <link href="~/AdminTemplates/css/icons.min.css" rel="stylesheet" type="text/css" />
    <!-- App Css-->
    <link href="~/AdminTemplates/css/app.min.css" id="app-style" rel="stylesheet" type="text/css" />
    <link href="~/admintemplates/libs/sweetalert2/sweetalert2.min.css" rel="stylesheet" />
</head>
<body data-topbar="dark">


    <div id="layout-wrapper">
        @await Component.InvokeAsync("Header")
        @await Component.InvokeAsync("SideBar")
        <div class="main-content">
            <div class="page-content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>
        </div>
    </div>
    <!-- JAVASCRIPT -->
    <script src="~/AdminTemplates/libs/jquery/jquery.min.js"></script>
    <script src="~/lib/jquery-validation/dist/jquery.validate.min.js"></script>
    <script src="~/lib/jquery-validation-unobtrusive/jquery.validate.unobtrusive.min.js"></script>
    <script src="~/AdminTemplates/libs/bootstrap/js/bootstrap.bundle.min.js"></script>
    <script src="~/AdminTemplates/libs/metismenu/metisMenu.min.js"></script>
    <script src="~/AdminTemplates/libs/simplebar/simplebar.min.js"></script>
    <script src="~/AdminTemplates/libs/node-waves/waves.min.js"></script>
    <script src="~/admintemplates/libs/sweetalert2/sweetalert2.min.js"></script>
    <!--Morris Chart-->

    <script src="~/AdminTemplates/libs/morris.js/morris.min.js"></script>
    <script src="~/AdminTemplates/libs/raphael/raphael.min.js"></script>
    <script src="~/AdminTemplates/libs/jquery-knob/jquery.knob.min.js"></script>
    <script src="~/AdminTemplates/libs/metrojs/release/MetroJs.Full/MetroJs.min.js"></script>

    <script src="~/AdminTemplates/js/pages/dashboard.init.js"></script>

    <script src="~/AdminTemplates/js/app.js"></script>
    

    @await RenderSectionAsync("scripts", false);
</body>
</html>
